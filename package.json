{
  "name": "fashionforge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "set PORT=3001 && nodemon server/index.js",
    "client": "cd client && npx vite --host 0.0.0.0 --port 5000",
    "build": "npm --prefix client ci --include=dev && npm --prefix client run build",
    "db:init": "node server/db/init.js",
    "preview": "cd client && vite preview --host 0.0.0.0 --port 5000",
    "test:vertex": "node server/test-vertex-ai.js"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.7.0",
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.12.2",
    "cloudinary": "^2.5.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^2.0.0",
    "pg": "^8.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.7"
  }
}

